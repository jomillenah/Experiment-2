#include <iostream>
#include <conio.h>

using namespace std; 
  
int main()
{
    int m,l;
    cout<<"How many Rows: ";
    cin>>m;
    cout<<"How many Columns: ";
    cin>>l;
    
    for (int n = 1; n <= m; n++)
    {
    for (int o = 1; o <= l; o++)
    {
    cout << "*";
    }
    cout<<endl;
    }
    _getch();
    return 0;
}
