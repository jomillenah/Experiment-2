#include<iostream>
#include <conio.h>

using namespace std;

int main()
{
	int J, A, Z;
	
	cout << "Enter first number: ";
	cin >> J;
	cout << "Enter second number: ";
	cin >> A;
	cout << "Enter third number: ";
	cin >> Z;

	if (J >= A && A >= Z)
	{
	cout << "\nThe largest number is " << J << ".";
	}
	
		else if (A >= J && A >= Z)
		{
		cout << "\nThe largest number is " << A << ".";
		}
	
			else
			{
			cout << "\nThe largest number is " << Z << ".";
			}
      
	_getch();
	return 0;
}
