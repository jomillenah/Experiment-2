#include <iostream>
#include <conio.h>

using namespace std;

int main()
{
    int x;

    cout << "Enter a number: ";
    cin >> x;
    cout<< "\nMultiplication table of " << x << ":";

    for (int i = 1; i <= 10; ++i) 
  	{
    cout << "\n" << x << " * " << i << " = " << x * i << endl;
    }
    
    _getch();
    return 0;
}
